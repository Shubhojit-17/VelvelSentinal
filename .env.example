# Velvet Sentinel Environment Configuration
# Copy to .env.local and fill in values

# Thirdweb Configuration
THIRDWEB_CLIENT_ID=your_client_id_here
THIRDWEB_SECRET_KEY=your_secret_key_here

# Phala dStack Configuration
DSTACK_SIMULATOR_ENDPOINT=http://localhost:8090

# Cortensor Configuration
CORTENSOR_API_KEY=your_cortensor_api_key
CORTENSOR_ENDPOINT=https://api.cortensor.network

# Server Wallet for x402 Payments
SERVER_WALLET_ADDRESS=0x0000000000000000000000000000000000000000
SERVER_WALLET_PRIVATE_KEY=

# RPC Endpoints
ETHEREUM_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/your-key
ARBITRUM_RPC_URL=https://arb-mainnet.g.alchemy.com/v2/your-key
ARBITRUM_SEPOLIA_RPC_URL=https://arb-sepolia.g.alchemy.com/v2/your-key

# IPFS Configuration
IPFS_GATEWAY=https://gateway.pinata.cloud
PINATA_API_KEY=
PINATA_SECRET_KEY=

# Dashboard
NEXT_PUBLIC_THIRDWEB_CLIENT_ID=your_client_id_here
NEXT_PUBLIC_APP_URL=http://localhost:3000
